{
	"name": "lasuillard/Libra",
	"dockerComposeFile": ["../docker-compose.yaml", "./docker-compose.override.yaml"],
	"runServices": ["workspace", "novnc"],
	"service": "workspace",
	"overrideCommand": true,
	"initializeCommand": "./.devcontainer/initializeCommand.sh",
	"onCreateCommand": "./.devcontainer/onCreateCommand.sh",
	"updateContentCommand": "./.devcontainer/updateContentCommand.sh",
	"postCreateCommand": "./.devcontainer/postCreateCommand.sh",
	"postStartCommand": "./.devcontainer/postStartCommand.sh",
	"postAttachCommand": "./.devcontainer/postAttachCommand.sh",
	"shutdownAction": "stopCompose",
	"workspaceFolder": "/workspaces/Libra",
	"customizations": {
		"vscode": {
			"extensions": [
				"ldez.ignore-files",
				"gruntfuggly.todo-tree",
				"redhat.vscode-yaml",
				"tamasfe.even-better-toml",
				"streetsidesoftware.code-spell-checker",
				"EditorConfig.EditorConfig",
				"vadimcn.vscode-lldb",
				"rust-lang.rust-analyzer",
				"vivaxy.vscode-conventional-commits",
				"cweijan.vscode-database-client2",
				"svelte.svelte-vscode",
				"tauri-apps.tauri-vscode",
				"jock.svg",
				"bradlc.vscode-tailwindcss",
				"ZixuanChen.vitest-explorer",
				"dbaeumer.vscode-eslint",
				"esbenp.prettier-vscode"
			]
		}
	}
}
